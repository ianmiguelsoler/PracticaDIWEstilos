.text-basic {
  font-family: 'Verdana, Arial, sans-serif';
  font-size: 12px;
  color: #3498db;
}
body {
  font-family: 'Verdana, Arial, sans-serif';
  font-size: 12px;
  color: #3498db;
  background-color: #217dbb;
}
.mapa_imagen {
  position: relative;
}
.mapa_imagen img {
  max-width: 100%;
  border-radius: 10px;
}
.mapa_imagen ul.notas li {
  list-style: none;
  display: none;
  position: absolute;
  border: medium solid white;
}
.mapa_imagen ul.notas li:hover p {
  display: block;
}
.mapa_imagen ul.notas li p {
  margin: 10px 0 0 0;
  padding: 0.3em;
  display: none;
  background: #8bc4ea;
  opacity: 0.75;
  position: absolute;
  top: 100%;
}
.sprite {
  background: url('./images/spritesheet.png') no-repeat;
}
.sprite.hover1 {
  background-position: 0 0;
  width: 120px;
  height: 130px;
  top: 50px;
  left: 240px;
}
.sprite.hover2 {
  background-position: -120px 0;
  width: 350px;
  height: 150px;
  top: 190px;
  left: 180px;
}
.sprite.hover3 {
  background-position: -470px 0;
  width: 100px;
  height: 60px;
  top: 350px;
  left: 400px;
}
#auroraB {
  width: 100%;
  max-width: 1200px;
}
.base64-img {
  background: url('data:image/png;base64,[TU_CODIGO_BASE64]') no-repeat;
  width: 100px;
  height: 100px;
}
